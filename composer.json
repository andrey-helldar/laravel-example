{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-curl": "^7.1",
        "babenkoivan/scout-elasticsearch-driver": "^3.10",
        "barryvdh/laravel-cors": "^0.11.3",
        "barryvdh/laravel-ide-helper": "^2.5",
        "darkaonline/l5-swagger": "5.8.*",
        "dillingham/nova-attach-many": "^1.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "garygreen/pretty-routes": "^1.0",
        "genealabs/nova-horizon": "^0.1.1",
        "genealabs/nova-telescope": "^0.1.3",
        "gregoriohc/laravel-nova-theme-responsive": "^0.6.2",
        "jedrzej/searchable": "^0.0.14",
        "laravel/framework": "5.8.*",
        "laravel/horizon": "^3.1",
        "laravel/nova": "*",
        "laravel/passport": "^7.0",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "nesbot/carbon": "~2.0",
        "php-junior/nova-logs": "^1.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.4",
        "rvkolosov/laravel-withtrait": "dev-master",
        "yassi/nova-nested-form": "^2.1",
        "zircote/swagger-php": "3.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "ext-pcntl": "7.3",
            "ext-posix": "7.3"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "koolreport\\laravel\\": "vendor/koolphp/koolreport/packages/laravel"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ]
}
